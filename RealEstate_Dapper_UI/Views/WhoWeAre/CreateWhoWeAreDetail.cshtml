@model CreateWhoWeAreDetailDto
@{
    ViewData["Title"] = "CreateWhoWeAreDetail";
    Layout = "~/Views/Admin/Index.cshtml";
}

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Create WhoWeAre Detail</h6>

                <form method="post">
                    <label>Title : </label>
                    <input type="text" asp-for="Title" class="form-control" />
                    <br />
                    <label>Subtitle : </label>
                    <input type="text" asp-for="Subtitle" class="form-control" />
                    <br />
                    <label>Description1 : </label>
                    <textarea rows="5" asp-for="Description1" class="form-control"></textarea>
                    <br />
                    <label>Description2 : </label>
                    <textarea rows="5" asp-for="Description2" class="form-control"></textarea>
                    <br />
                    <button class="btn btn-primary">Save Detail</button>
                </form>




            </div>
        </div>
    </div>
</div>

